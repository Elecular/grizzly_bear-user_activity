{
    "name": "app",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node --unhandled-rejections=strict ./bin/www",
        "start:dev": "./bin/dev.sh",
        "random-data-dump:dev": "node ./bin/data-dump/random.js",
        "test": "npm run test:jest && npm run test:acceptance",
        "test:jest": "./bin/jest-test.sh",
        "test:acceptance": "./bin/postman-test.sh"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "log4js": "^6.1.2",
        "md5": "2.2.1",
        "mongodb": "^3.5.5",
        "morgan": "~1.9.1",
        "recursive-readdir": "^2.2.2",
        "yargs": "^15.3.1"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-mocha": "^6.3.0",
        "husky": "^4.2.3",
        "jest": "^25.2.4",
        "lint-staged": "^10.0.8",
        "mocha": "^7.1.0",
        "newman": "^4.6.0",
        "nodemon": "^2.0.2",
        "prettier": "1.19.1",
        "wait-on": "^4.0.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "./node_modules/.bin/eslint",
            "prettier --write",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    }
}
